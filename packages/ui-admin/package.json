{
  "name": "ui-admin",
  "private": true,
  "version": "1.0.0",
  "description": "ui admin for racoon ui based bootstrap 4",
  "author": "we are racoons",
  "repository": {
    "type": "git",
    "url": "https://github.com/racoon-ui/groot.git"
  },
  "bugs": "https://github.com/racoon-ui/groot/issues",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "prestart": "yarn bundle",
    "start": "ui-scripts --server -p 8001",
    "start:watch": "ui-build --bundle --watch -p 8080",
    "bundle": "ui-build --bundle",
    "lint": "ui-test --lint",
    "lint:fix": "ui-test --lint --fix",
    "clean": "ui-build --clean"
  },
  "license": "MIT",
  "devDependencies": {
    "@racoon-ui/ui-babel-preset": "^1",
    "@racoon-ui/ui-polyfill-loader": "^1",
    "@racoon-ui/ui-webpack-config": "^1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
