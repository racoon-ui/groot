{
  "name": "racoon-ui",
  "description": "UI libraries made by Racoons of ST Unitas",
  "private": true,
  "version": "1.0.0",
  "author": "we are racoons",
  "repository": {
    "type": "git",
    "url": "https://github.com/racoon-ui/groot.git"
  },
  "bugs": "https://github.com/racoon-ui/groot/issues",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn bootstrap",
    "start": "lerna run start --stream",
    "start:watch": "lerna run start:watch --stream",
    "dev": "yarn start:watch",
    "clean:modules": "lerna clean --yes",
    "bootstrap": "npm-run-all clean:modules install:packages clean build",
    "build": "lerna run build --stream",
    "install:packages": "lerna bootstrap --reject-cycles",
    "clean": "lerna run clean --stream",
    "lint": "lerna run lint --stream",
    "lint:staged": "lerna exec --stream lint-staged"
  },
  "resolutions": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "yarn": "^1.15.2"
  },
  "engines": {
    "node": "^10",
    "yarn": ">=1.6.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.css": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged"
    }
  },
  "dependencies": {
    "eslint": "^5.16.0"
  }
}
